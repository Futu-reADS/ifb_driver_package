<launch>  
  <node pkg="ifb_driver_package" exec="ifb_driver_node" name="ifb_driver" output="log">
    <param name="BAUD_RATE"             value="115200" />
    <param name="IFBSTAT_TIMEOUT_NSEC"  value="1e9" />
    <param name="IFB_TIMEOUT_SEC"       value="0.5" />
    <param name="SERIAL_TIMEOUT"        value="1.0" />
    <param name="WRITE_TIMEOUT"         value="1.0" />
    <param name="ANGLE_STEER_DEG_MAX"   value="30" />
    <param name="WHEEL_BASE"            value="1.0" />
    <param name="ACC_CMD_FACTOR"        value="1000.0" />
    <param name="ACC_CMD_MIN"           value="-2000" />
    <param name="ACC_CMD_MAX"           value="3000" />
    <param name="STEER_CMD_MIN"         value="-500" />
    <param name="STEER_CMD_MAX"         value="500" />
    <param name="STEER_CMD_FACTOR"      value="1000.0" />
    <param name="TIMER_MILLI"           value="20" />
    
  </node>
</launch>